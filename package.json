{
  "name": "friendly-dates",
  "version": "1.0.3",
  "description": "Type-safe ISO-derived date keys for TypeScript with utilities for parsing, formatting, and converting between day, week, month, and year keys",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "prepublishOnly": "npm run build",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ci": "vitest run"
  },
  "keywords": [
    "date",
    "dates",
    "date-keys",
    "typescript",
    "type-safe",
    "iso",
    "formatting",
    "date-fns"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aaronbeall/friendly-dates.git"
  },
  "bugs": {
    "url": "https://github.com/aaronbeall/friendly-dates/issues"
  },
  "homepage": "https://github.com/aaronbeall/friendly-dates#readme",
  "peerDependencies": {
    "date-fns": "^2.0.0 || ^3.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "date-fns": "^3.3.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.0"
  }
}
